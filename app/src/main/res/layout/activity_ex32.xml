    android:background="@color/black"
    tools:context=".ex32Activity">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_marginVertical="0.5dp">
        <ImageView
            android:layout_marginHorizontal="5dp"
            android:layout_width="115dp"
            android:layout_height="122dp"
            android:src="@drawable/onibus"></ImageView>
        <ImageView
            android:layout_marginHorizontal="5dp"
            android:layout_width="115dp"
            android:layout_height="122dp"
            android:src="@drawable/aviao"></ImageView>
        <ImageView
            android:layout_marginHorizontal="5dp"
            android:layout_width="115dp"
            android:layout_height="122dp"
            android:src="@drawable/bairros"></ImageView>
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_marginVertical="0.5dp">
        <ImageView
            android:layout_marginHorizontal="5dp"
            android:layout_width="115dp"
            android:layout_height="122dp"
            android:src="@drawable/turismo"></ImageView>
        <ImageView
            android:layout_marginHorizontal="5dp"
            android:layout_width="115dp"
            android:layout_height="122dp"
            android:src="@drawable/telefone"></ImageView>
        <ImageView
            android:layout_marginHorizontal="5dp"
            android:layout_width="115dp"
            android:layout_height="122dp"
            android:src="@drawable/taxi"></ImageView>
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_marginVertical="0.5dp">
        <ImageView
            android:layout_marginHorizontal="5dp"
            android:layout_width="115dp"
            android:layout_height="122dp"
            android:src="@drawable/cultura"></ImageView>
        <ImageView
            android:layout_marginHorizontal="5dp"
            android:layout_width="115dp"
            android:layout_height="122dp"
            android:src="@drawable/feriados"></ImageView>
        <ImageView
            android:layout_marginHorizontal="5dp"
            android:layout_width="115dp"
            android:layout_height="122dp"
            android:src="@drawable/sac"></ImageView>
    </TableRow>

</TableLayout>